{
  "name": "service-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "dev:android": "turbo run dev:android",
    "dev:ios": "turbo run dev:ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fmt": "prettier --experimental-cli --check .",
    "fmt:fix": "prettier --experimental-cli --write .",
    "db:generate": "pnpm --filter @repo/database generate",
    "db:migrate": "pnpm --filter @repo/database migrate",
    "db:studio": "pnpm --filter @repo/database studio"
  },
  "packageManager": "pnpm@10.17.1",
  "devDependencies": {
    "@bastidood/eslint-plugin-imsort": "^0.10.1",
    "@eslint/js": "^9.36.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-expo": "^1.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "eslint-plugin-turbo": "^2.5.8",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  }
}
