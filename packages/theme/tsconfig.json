    "outDir": "dist",
    "declaration": true,
    "module": "esnext",
    "moduleResolution": "node10",
    "target": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
