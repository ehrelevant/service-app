{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleDetection": "auto",
    "moduleResolution": "node",
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-native",
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["src"]
}
